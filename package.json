{
  "name": "candidate-management",
  "description": "A candidate manager application",
  "author": "Christian SÃ¶derberg <hej@christiansoderberg.se>",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "create:db": "touch db.json && echo '{\"candidates\": []}' > db.json",
    "start:api": "tsx watch  --env-file=.env backend/index.ts",
    "start:db": "json-server db.json --port 3002",
    "start": "npx concurrently \"npm run start:api\" \"npm run start:db\" \"npm run dev\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.40.0",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.13",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "json-server": "^1.0.0-beta.0",
    "tsx": "^4.11.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
